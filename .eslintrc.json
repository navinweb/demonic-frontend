{
    "parser": "babel-eslint",
    "env": {
        "node": true,
        "es6": true,
        "jest": true,
        "browser": true,
        "jasmine": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "globals": {
        "AppError": true
    },
    "extends": [
        "eslint:recommended",
        "google"
    ],
    "rules": {
        "no-console": 0,
        "require-jsdoc": 0,
        "no-invalid-this": 0,
        "comma-dangle": 1,
        "max-len": 1,
        "no-unused-vars": 0,
        "object-curly-spacing": [
            "error",
            "always"
        ]
    }
}
